//.*: oneLineComment
/\*(.|[\n])*\*/: multiLineComment
".*": string
\bimport\s: import
\bclass\b: class
\bpublic\b|\bprivate\b|\bstatic\b: quali
\bvoid\b: void
\bextends\b: extends
\binterface\b: interface
implements: implements
\bint\b|\blong\s+int\b|\blong\b|\bchar\b|\bshort\b|\bfloat\b|\bdouble\b: types
\bString\b: StringType
\d+: digit
\bwhile\b: while
\bfor\b: for
\bif\b|\belse\s+if\b|\belse\b: ifelse
\breturn\b: return
\bbreak\b: break
\+\+|-|\+|\*|/: stuff
