//.*: oneLineComment
/\*(.|[\n])*\*/: multiLineComment
".*": string
#\s*include\s+((<.*>)|(".*")): include
\bint\b|long\s+int\b|\blong\b|\bchar\b|\bshort\b|\bfloat\b|\bdouble\b|\bunsigned\b: types
\d+: digit
#define\s+\w*\b: macro
\bwhile\b: while
\bfor\b: for
\bif\b|\belse\s+if\b|\belse\b: ifelse
\bgoto\b: goto
\+\+|-|\+|\*|/: stuff
