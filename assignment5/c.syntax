//.*: oneLineComment
/\*(.|[\n])*\*/: multiLineComment
".*": string
#\s*include\s+((<.*>)|(".*")): include
int\s|long\s+int\s|long\s|char\s|short\s|float\s|double\s|unsigned\s: types
\d+: digit
#define\s+\w*\s+: macro
while: while
for: for
if|else\s+if|else: ifelse
goto|(([a-z][A-Z])+): goto
\+\+|-|\+|\*|/: stuff
