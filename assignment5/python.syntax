#.*: oneLineComment
"""(.|[\n])*""": multiLineComment
\bdef[ \t]\w+: funcDef
".*"|'.*': string
\bimport\b|\bfrom\b: import
\bNone\b|\bTrue\b|\bFalse\b: special
\bfor\b|\bin\b: forloop
\bwhile\b: whileloop
\btry\b|\bexcept\b: tryExcept
\bif\b|\belse\b|\belif\b: ifelseBlock
\bpass\b: passStmt
\bas\b: assStmt
@.*: decorator
==|!=|=<|=>|<|>: compOp
=: assignment
\d+: digit
